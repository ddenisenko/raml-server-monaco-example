{
  "name": "raml-server-monaco-example",
  "version": "0.0.1",
  "description": "Example client for RAML Server",
  "main": "./dist/index.js",
  "scripts": {
    "build": "rimraf dist && tsc && webpack --progress --config webpack.config.js && cp -r node_modules/monaco-editor/min/vs docs",
    "run" : "npm run build && node_modules/.bin/http-server -c-1 ./ -p 8888"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raml-org/raml-language-server.git"
  },
  "keywords": [
    "raml"
  ],
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ddenisenko/raml-server-monaco-example/issues"
  },
  "homepage": "https://github.com/ddenisenko/raml-server-monaco-example#readme",
  "typings": "./dist/index.d.ts",
  "dependencies": {
    "raml-language-server-browser":"0.0.2",
    "bootstrap-treeview":"*"
  },
  "devDependencies": {
    "monaco-editor": "^0.8.3",
    "typescript": "^2.2",
    "http-server": "^0.9.0",
    "webpack": "1.4.2",
    "rimraf": "*",
    "vscode-languageserver-types": "^3.0.2-beta.5",
    "@types/es6-promise": "*",
    "@types/jquery": "2.0.47"
  },
  "peerDependencies": {
    "monaco-editor": "^0.8.3"
  },
  "engines": {
    "node": "*",
    "vscode": "*"
  }
}
